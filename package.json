{
  "name": "gorkie-slack",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A helpful AI Slack Bot built with AI SDK.",
  "private": true,
  "scripts": {
    "dev": "bun run --watch server/index.ts",
    "start": "bun run server/index.ts",
    "sandbox:build": "bun run server/scripts/build-template.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "dbgenerate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "check": "ultracite check .",
    "check:spelling": "cspell -c .cspell.jsonc --no-progress --no-summary --no-must-find-files --unique --cache",
    "check:unsafe": "ultracite fix --unsafe true .",
    "check:write": "ultracite fix .",
    "typecheck": "tsc --noEmit",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@e2b/code-interpreter": "^2.3.3",
    "@langfuse/otel": "^4.6.1",
    "@mariozechner/pi-coding-agent": "^0.54.0",
    "@neondatabase/serverless": "^1.0.2",
    "@openrouter/ai-sdk-provider": "^2.2.3",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@sinclair/typebox": "^0.34.48",
    "@slack/bolt": "^4.6.0",
    "@t3-oss/env-core": "^0.13.10",
    "ai": "^6.0.97",
    "ai-retry": "^1.2.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "e2b": "^2.12.1",
    "exa-js": "^2.4.0",
    "p-queue": "^9.1.0",
    "pako": "^2.1.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "sanitize-filename": "^1.6.3",
    "strip-ansi": "^7.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@commitlint/types": "^20.3.1",
    "@cspell/dict-bash": "^4.2.2",
    "@types/bun": "^1.3.9",
    "@types/pako": "^2.0.4",
    "cspell": "^9.6.0",
    "drizzle-kit": "^0.31.9",
    "lefthook": "^2.1.1",
    "typescript": "^5.9.3",
    "ultracite": "7.2.3"
  }
}
