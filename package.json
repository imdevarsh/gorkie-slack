{
  "name": "gorkie-slack",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A helpful AI Slack Bot built with AI SDK.",
  "private": true,
  "scripts": {
    "dev": "bun run --watch server/index.ts",
    "start": "bun run server/index.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "dbgenerate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "check": "ultracite check .",
    "check:spelling": "cspell -c .cspell.jsonc --no-progress --no-summary --no-must-find-files --unique --cache",
    "check:unsafe": "ultracite fix --unsafe true .",
    "check:write": "ultracite fix .",
    "typecheck": "tsc --noEmit",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.26",
    "@daytonaio/sdk": "^0.143.0",
    "@exalabs/ai-sdk": "^1.0.5",
    "@langfuse/otel": "^4.5.1",
    "@langfuse/tracing": "^4.5.1",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@neondatabase/serverless": "^1.0.2",
    "@openrouter/ai-sdk-provider": "^2.0.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@slack/bolt": "^4.6.0",
    "@slack/types": "^2.19.0",
    "@t3-oss/env-core": "^0.13.10",
    "@types/pako": "^2.0.4",
    "ai": "^6.0.48",
    "ai-retry": "^1.0.2",
    "drizzle-orm": "^0.45.1",
    "p-queue": "^9.1.0",
    "pako": "^2.1.0",
    "pino": "^10.2.1",
    "pino-pretty": "^13.1.3",
    "sandbox-agent": "^0.2.0",
    "sanitize-filename": "^1.6.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.12",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@commitlint/types": "^20.3.1",
    "@cspell/dict-bash": "^4.2.2",
    "@types/bun": "^1.3.6",
    "cspell": "^9.6.0",
    "drizzle-kit": "^0.31.9",
    "lefthook": "^2.0.15",
    "typescript": "^5.9.3",
    "ultracite": "7.0.12"
  }
}
